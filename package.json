{
  "name": "twilio-taskrouter-worker",
  "title": "Twilio TaskRouter Worker",
  "version": "2.0.0-dev",
  "description": "Twilio TaskRouter Worker JavaScript library",
  "homepage": "https://twilio.com",
  "author": "Wanjun Li <wli@twilio.com>",
  "license": "MIT",
  "keywords": [
    "twilio",
    "library",
    "javascript",
    "taskrouter",
    "worker"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint ./lib",
    "test:unit": "mocha ./test/unit/index.js",
    "test:integration": "mocha ./test/integration/index.js",
    "test": "run-s test:unit test:integration",
    "build:js": "node ./scripts/build.js ./src/twilio-taskrouter-worker.js ./LICENSE.md ./dist/twilio-taskrouter-worker.js",
    "build:min.js": "uglifyjs ./dist/twilio-taskrouter-worker.js -o ./dist/twilio-taskrouter-worker.min.js --comments \"/^! twilio-taskrouter-worker.js/\"",
    "build": "npm-run-all clean lint docs cover test:integration build:js build:min.js",
    "docs": "node ./scripts/docs.js ./dist/docs",
    "clean": "rimraf ./coverage ./dist",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./test/unit/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://code.hq.twilio.com/wli/twilio-taskrouter-worker.js.git"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "events": "^1.1.1",
    "loglevel": "^1.4.1",
    "ws": "^2.0.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "chai-as-promised": "^6.0.0",
    "chai-datetime": "^1.4.1",
    "cheerio": "^0.22.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "mocha": "^3.1.2",
    "npm-run-all": "^4.0.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "twilio": "^2.11.1",
    "uglify-js": "^2.7.5",
    "utf-8-validate": "^3.0.1",
    "vinyl-fs": "^2.4.4",
    "vinyl-source-stream": "^1.1.0"
  },
  "browser": {
    "ws": "./src/ws.js",
    "xmlhttprequest": "./src/xmlhttprequest.js"
  }
}
